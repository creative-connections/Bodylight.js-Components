{
  "name": "aurelia-bodylight-plugin",
  "description": "Bodylight plugin for Aurelia framework",
  "version": "2.1.33",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/creative-connections/aurelia-bodylight-plugin.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.13.1",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.13.5",
    "@babel/register": "^7.13.0",
    "@babel/eslint-parser": "^7.17.0",
    "@danzen/createjs": "^1.3.2",
    "aurelia-animator-css": "^1.0.4",
    "aurelia-cli": "^2.0.3",
    "aurelia-framework": "^1.3.1",
    "aurelia-loader-nodejs": "^1.1.0",
    "aurelia-pal-nodejs": "^2.0.0",
    "aurelia-testing": "^1.0.0",
    "aurelia-tools": "^2.0.0",
    "aurelia-web-components": "git+https://github.com/TomasKulhanek/web-components.git",
    "babel-jest": "^26.3.0",
    "connect": "^3.7.0",
    "connect-history-api-fallback": "^1.6.0",
    "connect-injector": "^0.4.4",
    "debounce": "^1.2.0",
    "del": "^5.1.0",
    "eslint": "^8.13.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-cache": "^1.1.3",
    "gulp-eslint": "^6.0.0",
    "gulp-if": "^3.0.0",
    "gulp-plumber": "^1.2.1",
    "gulp-rename": "^2.0.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-watch": "^5.0.1",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^26.4.0",
    "jest-cli": "^26.4.0",
    "jest-transform-stub": "^2.0.0",
    "minimatch": "^3.0.4",
    "open": "^7.4.2",
    "promise-polyfill": "^8.1.3",
    "regenerator-runtime": "^0.13.7",
    "requirejs": "^2.3.6",
    "serve-static": "^1.14.1",
    "server-destroy": "^1.0.1",
    "text": "github:requirejs/text"
  },
  "backupdependencies": {
    "aurelia-web-components": "waiting for official release which will involve PR https://github.com/aurelia/web-components/pull/26"
  },
  "browserslist": [
    "defaults"
  ],
  "scripts": {
    "build": "au build-plugin --env prod",
    "start": "au run",
    "watch": "au build-plugin --watch",
    "prepare": "npm run build",
    "pretest": "au lint",
    "preversion": "au test",
    "test": "au test"
  },
  "engines": {
    "node": ">=10.12.0"
  },
  "main": "dist/commonjs/index.js",
  "module": "dist/native-modules/index.js",
  "private": false,
  "files": [
    "dist",
    "src",
    "animate"
  ],
  "jest": {
    "moduleNameMapper": {
      "^aurelia-binding$": "<rootDir>/node_modules/aurelia-binding"
    },
    "modulePaths": [
      "<rootDir>/src",
      "<rootDir>/node_modules"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.(css|less|sass|scss|styl|jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "jest-transform-stub",
      "^.+\\.js$": "babel-jest"
    },
    "testRegex": "\\.spec\\.js$",
    "setupFiles": [
      "<rootDir>/test/jest-pretest.js"
    ],
    "testEnvironment": "node",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!**/*.spec.js",
      "!**/node_modules/**",
      "!**/test/**"
    ],
    "coverageDirectory": "<rootDir>/test/coverage-jest",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "html"
    ]
  },
  "dependenciesbackup": {
    "aurelia-web-components": "^1.0.0-beta.3"
  },
  "dependencies": {
    "aurelia-bootstrapper": "^2.3.2",
    "aurelia-dynamic-html": "^0.3.1",
    "aurelia-event-aggregator": "^1.0.3",
    "aurelia-fetch-client": "^1.8.2",
    "aurelia-http-client": "^1.3.1",
    "aurelia-i18n": "^3.1.1",
    "aurelia-loader": "^1.0.2",
    "aurelia-templating": "^1.10.4",
    "canvas": "2.6.1",
    "chart.js": "^2.9.4",
    "chartjs-plugin-datalabels": "^0.7.0",
    "createjs": "^1.0.1",
    "elementary-circuits-directed-graph": "^1.3.1",
    "file-saver": "^2.0.2",
    "font-awesome": "^4.7.0",
    "gifffer": "^1.5.4",
    "highlight.js": "^10.7.2",
    "katex": "^0.12.0",
    "latest-createjs": "^1.0.24",
    "libgif": "0.0.3",
    "lodash": "^4.17.21",
    "markdown-it": "^12.0.4",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-for-inline": "^0.1.1",
    "markdown-it-katexx": "^3.2.0",
    "pdbe-molstar": "git+https://github.com/TomasKulhanek/pdbe-molstar.git"
  },
  "bugs": {
    "url": "https://github.com/creative-connections/aurelia-bodylight-plugin/issues"
  },
  "homepage": "https://github.com/creative-connections/aurelia-bodylight-plugin#readme",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "keywords": [
    "aurelia",
    "bodylight",
    "fmu",
    "webcomponents",
    "modelica",
    "webassembly"
  ],
  "author": "Tomas Kulhanek"
}
